
    <h1>Create an Note</h1>

    <%= form_for @note do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :date %><br>
      <%= f.date_field :date %>
    </p>
    <p>
      <%= f.label :instructor %><br>
      <%= f.text_field :instructor %>
    </p>
    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.submit %>
    </p>
    <% end %>
