<div class="note-show-container">
<div class="note-show-box">
    <h1>New Note</h1>

<%= form_for @note do |f| %>
<table class="table">
  <thead>
    <tr>

    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row"><%= f.label :title %></th>
      <td><%= f.text_field :title %></td>
    </tr>
    <tr>
      <th scope="row"><%= f.label :date %></th>
      <td><%= f.date_field :date %></td>
    </tr>
    <tr>
      <th scope="row"><%= f.label :instructor %></th>
      <td><%= f.text_field :instructor %></td>
    </tr>
    <tr>
      <th scope="row"><%= f.label :description %></th>
      <td><%= f.text_field :description %></td>
    </tr>
  </tbody>
</table>
 <p>
      <%= f.submit %>
    </p>
<% end %>