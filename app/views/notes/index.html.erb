<div class="note-show-container">
<div class="note-show-box">
<h1>All Notes</h1>
<table class="table table-hover">
  <tr>
    <th>Date</th>
    <th>Title</th>
    <th>Instructor</th>
    <th>Description</th>
  </tr>
  <% @user_notes.each do |note| %>
  <tr>
    <td><%= note.date %></td>
    <td><%= note.title %></td>
    <td><%= note.instructor %></td>
    <td><%= note.description %></td>
  </tr>
  <% end %>
</table>
</div>
</div>